// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Review{
  id Int @default(autoincrement()) @id
  name String
  role String
  date Int
  avatar String
  peopleReview String
}